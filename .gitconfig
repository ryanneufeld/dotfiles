[user]
  name = Ryan Neufeld
[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  pd = !~/jquery/pretty-diff/pretty-diff.js
  gd = !~/jquery/pretty-diff/gist-diff.js
  g = grep -I
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ci) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --all
  pb = publish-branch
  sm = show-merges
  st = status
  ksdiff = difftool -y -t Kaleidoscope
  remotes = remote -v
  tags = tags -l
  branches = branch -l
[color]
  ui = auto
# [color "branch"]
#   current = yellow reverse
#   local = yellow
#   remote = green
# [color "diff"]
#   meta = yellow bold
#   frag = magenta bold
#   old = red bold
#   new = green bold
# [color "status"]
#   added = yellow
#   changed = green
#   untracked = cyan
[merge]
  log = true
[rerere]
  enabled = 1
[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
  insteadOf = "github:"
[url "git@gist.github.com:"]
  insteadOf = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
  insteadOf = "gist:"
[url "git@heroku.com:"]
  insteadOf = "heroku:"
[diff "exif"]
  textconv = exiftool
[core]
  autocrlf = input
  safecrlf = false
  editor = subl -w
[help]
  autocorrect = 1
[credential]
  helper = osxkeychain